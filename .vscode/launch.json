{
   "version": "0.2.0",
   "configurations": [
      {
         "name": "Debug libCloudundancyTests",
         "type": "cppdbg",
         "request": "launch",
         "cwd": "${workspaceFolder}",
         "program": "${workspaceRoot}/Debug/libCloudundancyTests/libCloudundancyTests",
         //"args": [ "" ],
         "externalConsole": false,
         "internalConsoleOptions": "openOnSessionStart",
         "logging": {
            "engineLogging": false,
            "trace": false,
            "traceResponse": false,
            "programOutput": false,
            "exceptions": false,
            "moduleLoad": true,
            "natvisDiagnostics": false
         },
         "MIMode": "gdb",
         "miDebuggerPath": "/usr/bin/gdb",
         "miDebuggerArgs": "--quiet",
         "preLaunchTask": "Build Debug",
         "stopAtEntry": false,
         "symbolLoadInfo": {
            "loadAll": true,
            "exceptionList": ""
         },
         
      }
